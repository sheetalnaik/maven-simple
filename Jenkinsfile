pipeline {
   agent any

   stages {
      stage('Hello') {
         steps {
            echo 'Hello World'
         }
      }
   
   
      stage('Hi') {
         steps {
            echo 'How are you'
         }
      }
   
 
      stage('Good') {
         steps {
            echo 'Doing good'
         }
      }
   }
}
