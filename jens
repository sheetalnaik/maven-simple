pipeline {
   agent any
      stages {
        stage("Publish to Nexus Repository Manager") {
            steps {
                        nexusArtifactUploader artifects:[
						[
                                artifactId: 'hello-world-war',
                                classifier: '',
                                file: '/var/lib/jenkins/workspace/job_for_pipeline/target/hello-world-war-1.0.0.war',
                                type: 'war'
                        ]
                        ],
		nexusVersion: 'nexus3',
		credentialsId: 'nexus credentails',
		groupId: 'com.efsavage',
		nexusUrl: 'localhost:8081',                  
		protocol: 'http',					
		repository: 'http://localhost:8081/repository/newnexusrepo/',	
		version: '1.0.0'		

				}
            }
        }
}
